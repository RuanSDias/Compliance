    <element xsi:type="archimate:Stakeholder" name="Presidente da software house(Dilan)" id="id-34bc91ca4a704a728c33c16f9d2c4fe7"/>
    <element xsi:type="archimate:Driver" name="Não podem mais ocorres atendimentos de suporte de maneira informal, pois clientes estão cancelando contratos com nossa empresa por insatisfação com o atendimento" id="id-4548ef8f5c61448384af3dea0a08ab98"/>
    <element xsi:type="archimate:Value" name="Aumentará a retenção de clientes (renovação de contratos). Prerdemos, em média, R$ 120.000,00 de contratos cancelados por insatisfação com o suporte técnico aos nossos produtos" id="id-38090e9ed4a042939e446c6ec8a01a93"/>
    <element xsi:type="archimate:Value" name="Um clinete satisfeito amplia negócios e indica novos clientes. Ampliamos R$ 280.000,00 por ano do nosso faturamento com as ampliações de negócios partindo de indicações dos clientes satisfeitos" id="id-5181f5269878452fba37c417d863ea0f"/>
    <element xsi:type="archimate:Assessment" name="Registro e acompanhamento dos atendimentos até o encerramento, usando um sistema digital (informatizado)" id="id-e0e7ca3c920a4d3c8d91781d3af71fee"/>
    <element xsi:type="archimate:Assessment" name="Ter um meio de acompanhar o grau de satisfação dos clientes com o suporte, de maneira formal" id="id-2593c567f44743509187f4dd0d16de80"/>
